{
    "name": "template-api-call-next",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "next": "next",
        "dev": "nodemon",
        "test": "jest",
        "pretty": "npx prettier --write .",
        "build": "next build && tsc --project tsconfig.server.json",
        "start": "cross-env NODE_ENV=production node dist/index.js",
        "lint": "npm run next lint",
        "loaddevdata": "docker exec -i mysql_container sh -c 'exec mysql database -uroot -p\"$MYSQL_ROOT_PASSWORD\"' < ./db.sql",
        "loadpgdevdata": "docker exec -i postgres_container sh -c 'exec psql database --username\"$POSTGRES_USER\" -p\"$POSTGRES_PORT\"' < ./db.sql",
        "mysql_shell": "docker exec -it mysql_container sh -c 'exec mysql database -uroot -p\"$MYSQL_PASSWORD\"'"
    },
    "dependencies": {
        "@typescript-eslint/parser": "^5.38.1",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "next": "12.3.1",
        "pg": "^8.8.0",
        "prettier": "^2.7.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "serverless-mysql": "^1.5.4",
        "swr": "^1.3.0"
    },
    "devDependencies": {
        "@nestjs/testing": "^9.1.4",
        "@types/jest": "^29.1.2",
        "@types/node": "^18.7.23",
        "@types/pg": "^8.6.5",
        "@types/react": "^18.0.18",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "eslint": "^8.24.0",
        "eslint-config-next": "12.3.1",
        "eslint-plugin-testing-library": "^5.7.2",
        "jest": "^29.2.0",
        "nodemon": "^2.0.19",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.7.4"
    }
}
